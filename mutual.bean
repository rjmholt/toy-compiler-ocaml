proc f(val int x)
    if x < 1 then
        write "f: Done\n";
    else
        write "f: x = ";
        write x;
        write "\n";
        g(x - 1);
    fi
end

proc g(val int x)
    if x < 1 then
        write "g: Done\n";
    else
        write "g: x = ";
        write x;
        write "\n";
        f(x - 1);
    fi
end

proc main()
    f(10);
end
