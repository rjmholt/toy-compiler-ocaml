proc main()
    bool x;
    int y;
    int z;

    write "Do you wish to proceed?[true/false] ";
    read x;
    write "\n";

    if x then
        y := 64;
        z := 11;
        write "y + z == ";
        write y + z;
        write "\n";
    else
        write "You have elected not to proceed\n";
    fi
end
